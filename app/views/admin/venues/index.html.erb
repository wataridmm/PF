<div class="container p-5">
    <table class="table table-striped">
        <thead>
            <tr>
                <th>会場ID</th>
                <th>会場名</th>
                <!--県名とか-->
                <th>最寄駅</th>
                <th>会場説明</th>
                <th>ステータス</th>
                <th></th>
            </tr>
        </thead>
        <% @venues.each do |venue| %>
        <tbody>
            <tr>
                <td><%= venue.id %></td>
                <td><%= venue.name %></td>
                <td><%= venue.station %></td>
                <td><%= venue.introduction %></td>
                <td><%= venue.status %></td>
                <td><%= link_to "詳細", admin_venue_path(venue.id) %></td>
            </tr>
        </tbody>
        <% end %>
    </table>
</div>

